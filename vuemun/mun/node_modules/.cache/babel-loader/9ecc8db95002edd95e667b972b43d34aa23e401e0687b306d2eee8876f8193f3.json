{"ast":null,"code":"import axios from \"axios\";\nimport Swal from \"sweetalert2\";\n// import axiosInstance from \"./axios/axiosinstance\";\n\nexport default {\n  name: \"App\",\n  components: {},\n  data() {\n    return {\n      sign: 0,\n      name: \"\",\n      email: \"\",\n      password: \"\",\n      role: \"\"\n    };\n  },\n  methods: {\n    showSign(bool) {\n      this.sign = bool;\n    },\n    signin(event) {\n      event.preventDefault();\n      axios.post(\"/api/login\", {\n        email: this.email,\n        password: this.password\n      }).then(response => {\n        console.log(response);\n        var accessToken = response.headers.access;\n        localStorage.setItem(\"access\", accessToken);\n        // 로그인 성공 후 받은 access 토큰과 사용자 정보 저장\n      }).catch(error => {\n        // 요청 오류가 있는 경우 작업 수행\n        if (error.response.status === 401) {\n          Swal.fire({\n            title: \"로그인 오류\",\n            text: \"다시 로그인 하셈\",\n            icon: \"warning\"\n          });\n        }\n      });\n    },\n    signup(event) {\n      event.preventDefault();\n      axios.post(\"/api/user/signup\", {\n        name: this.name,\n        email: this.email,\n        password: this.password,\n        role: \"USER\"\n      }).then(response => {\n        console.log(\"성공\", response);\n      }).catch(error => {\n        console.error(\"실패\", error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","Swal","name","components","data","sign","email","password","role","methods","showSign","bool","signin","event","preventDefault","post","then","response","console","log","accessToken","headers","access","localStorage","setItem","catch","error","status","fire","title","text","icon","signup"],"sources":["/Users/mungdori00/mun/vuemun/mun/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"wrapper\" >\n    <div class=\"login-container\" :class=\"{ 'right-panel-active': sign === 1 }\">\n      <div class=\"sign-up-container\">\n        <form>\n          <h1>Create Account</h1>\n          <div class=\"social-links\">\n            <div>\n              <a href=\"#\"><i class=\"fa fa-facebook\" aria-hidden=\"true\"></i></a>\n            </div>\n            <div>\n              <a href=\"#\"><i class=\"fa fa-twitter\" aria-hidden=\"true\"></i></a>\n            </div>\n            <div>\n              <a href=\"#\"><i class=\"fa fa-linkedin\" aria-hidden=\"true\"></i></a>\n            </div>\n          </div>\n          <span>or use your email for registration</span>\n          <input type=\"text\" placeholder=\"Name\" v-model=\"name\" />\n          <input type=\"email\" placeholder=\"Email\" v-model=\"email\" />\n          <input type=\"password\" placeholder=\"Password\" v-model=\"password\" />\n          <button class=\"form_btn\" @click=\"signup\">Sign Up</button>\n        </form>\n      </div>\n      <div class=\"sign-in-container\">\n        <form>\n          <h1>Sign In</h1>\n          <div class=\"social-links\">\n            <div>\n              <a href=\"#\"><i class=\"fa fa-facebook\" aria-hidden=\"true\"></i></a>\n            </div>\n            <div>\n              <a href=\"#\"><i class=\"fa fa-twitter\" aria-hidden=\"true\"></i></a>\n            </div>\n            <div>\n              <a href=\"#\"><i class=\"fa fa-linkedin\" aria-hidden=\"true\"></i></a>\n            </div>\n          </div>\n          <span>or use your account</span>\n          <input type=\"email\" placeholder=\"Email\" v-model=\"email\" />\n          <input type=\"password\" placeholder=\"Password\" v-model=\"password\" />\n          <button class=\"form_btn\" @click=\"signin\">Sign In</button>\n        </form>\n      </div>\n      <div class=\"overlay-container\">\n        <div class=\"overlay-left\">\n          <h1>Welcome Back</h1>\n          <p>To keep connected with us please login with your personal info</p>\n          <button id=\"signIn\" @click=\"showSign(0)\" class=\"overlay_btn\">\n            Sign In\n          </button>\n        </div>\n        <div class=\"overlay-right\">\n          <h1>Hello, Friend</h1>\n          <p>Enter your personal details and start journey with us</p>\n          <button id=\"signUp\" @click=\"showSign(1)\" class=\"overlay_btn\">\n            Sign Up\n          </button>\n        </div>\n      </div>\n    </div>\n  </div>\n  <div v-if=\"authentication== true\">\n      {{ username }}\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Swal from \"sweetalert2\";\n// import axiosInstance from \"./axios/axiosinstance\";\n\nexport default {\n  name: \"App\",\n  components: {},\n  data() {\n    return {\n      sign: 0,\n      name: \"\",\n      email: \"\",\n      password: \"\",\n      role: \"\",\n      \n\n    };\n  },\n  methods: {\n    showSign(bool) {\n      this.sign = bool;\n    },\n    signin(event) {\n      event.preventDefault();\n      axios\n        .post(\"/api/login\", {\n          email: this.email,\n          password: this.password,\n        })\n        .then((response) => {\n          console.log(response)\n          var accessToken = response.headers.access;\n          localStorage.setItem(\"access\", accessToken);\n          // 로그인 성공 후 받은 access 토큰과 사용자 정보 저장\n      \n        })\n        .catch((error) => {\n          // 요청 오류가 있는 경우 작업 수행\n        if (error.response.status === 401) {\n            Swal.fire({\n              title: \"로그인 오류\",\n              text: \"다시 로그인 하셈\",\n              icon: \"warning\",\n            });\n          }\n        });\n    },\n    signup(event) {\n      event.preventDefault();\n      axios\n        .post(\"/api/user/signup\", {\n          name: this.name,\n          email: this.email,\n          password: this.password,\n          role: \"USER\",\n        })\n        .then((response) => {\n          console.log(\"성공\", response);\n        })\n        .catch((error) => {\n          console.error(\"실패\", error);\n        });\n    },\n  },\n};\n</script>\n\n<style>\n@import \"./assets/all.css\";\n</style>\n "],"mappings":"AAoEA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,aAAa;AAC9B;;AAEA,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE,CAAC,CAAC;EACdC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,CAAC;MACPH,IAAI,EAAE,EAAE;MACRI,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE;IAGR,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAACC,IAAI,EAAE;MACb,IAAI,CAACN,IAAG,GAAIM,IAAI;IAClB,CAAC;IACDC,MAAMA,CAACC,KAAK,EAAE;MACZA,KAAK,CAACC,cAAc,CAAC,CAAC;MACtBd,KAAI,CACDe,IAAI,CAAC,YAAY,EAAE;QAClBT,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,QAAQ,EAAE,IAAI,CAACA;MACjB,CAAC,EACAS,IAAI,CAAEC,QAAQ,IAAK;QAClBC,OAAO,CAACC,GAAG,CAACF,QAAQ;QACpB,IAAIG,WAAU,GAAIH,QAAQ,CAACI,OAAO,CAACC,MAAM;QACzCC,YAAY,CAACC,OAAO,CAAC,QAAQ,EAAEJ,WAAW,CAAC;QAC3C;MAEF,CAAC,EACAK,KAAK,CAAEC,KAAK,IAAK;QAChB;QACF,IAAIA,KAAK,CAACT,QAAQ,CAACU,MAAK,KAAM,GAAG,EAAE;UAC/B1B,IAAI,CAAC2B,IAAI,CAAC;YACRC,KAAK,EAAE,QAAQ;YACfC,IAAI,EAAE,WAAW;YACjBC,IAAI,EAAE;UACR,CAAC,CAAC;QACJ;MACF,CAAC,CAAC;IACN,CAAC;IACDC,MAAMA,CAACnB,KAAK,EAAE;MACZA,KAAK,CAACC,cAAc,CAAC,CAAC;MACtBd,KAAI,CACDe,IAAI,CAAC,kBAAkB,EAAE;QACxBb,IAAI,EAAE,IAAI,CAACA,IAAI;QACfI,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,IAAI,EAAE;MACR,CAAC,EACAQ,IAAI,CAAEC,QAAQ,IAAK;QAClBC,OAAO,CAACC,GAAG,CAAC,IAAI,EAAEF,QAAQ,CAAC;MAC7B,CAAC,EACAQ,KAAK,CAAEC,KAAK,IAAK;QAChBR,OAAO,CAACQ,KAAK,CAAC,IAAI,EAAEA,KAAK,CAAC;MAC5B,CAAC,CAAC;IACN;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}