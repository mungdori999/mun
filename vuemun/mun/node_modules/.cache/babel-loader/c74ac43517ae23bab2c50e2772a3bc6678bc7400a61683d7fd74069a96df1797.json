  this.$store.commit('initUserInfo')\n                localStorage.removeItem('access');\n            }\n        })\n        \n    }\n  },\n};\n</script>\n\n<style>\n.sidebar {\n  position: fixed;\n  top: 0;\n  left: -250px;\n  height: 100%;\n  width: 250px;\n  background-color: #343a40;\n  transition: left 0.3s ease;\n  z-index: 1000;\n}\n.sidebar.active {\n  left: 0;\n}\n.sidebar .nav-link {\n  color: #fff;\n}\n.content {\n  padding-left: 15px;\n  padding-top: 15px;\n}\n.hamburger {\n  position: fixed;\n  top: 15px;\n  left: 15px;\n  font-size: 1.5rem;\n  cursor: pointer;\n  transition: left 0.3s ease;\n  z-index: 1001; /* 사이드바보다 위에 표시되도록 설정 */\n}\n.hamburger.active {\n  left: 265px; /* 사이드바 너비(250px) + 기본 여백(15px) */\n}\n</style>"],"mappings":"AAsBA,OAAOA,aAAY,MAAO,wBAAwB;AAGlD,eAAe;EACbC,UAAU,EAAE,CAAC,CAAC;EACdC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAAA,EAAG;MACd,IAAI,CAACF,eAAc,GAAI,CAAC,IAAI,CAACA,eAAe;IAC9C,CAAC;IAEDG,MAAMA,CAACC,KAAK,EAAE;MACVA,KAAK,CAACC,cAAc,CAAC,CAAC;MACtBR,aAAa,CAACS,GAAG,CAAC,kBAAkB,EACnCC,IAAI,CAAEC,QAAQ,IAAK;QAChB,IAAGA,QAAO,KAAM,GAAG,EAAE;UACjB,IAAI,CAACC,MAAM,CAACC,MAAM,CAAC,cAAc;UACjCC,YAAY,CAACC,UAAU,CAAC,QAAQ,CAAC;QACrC;MACJ,CAAC;IAEL;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}