{"ast":null,"code":"import axios from \"axios\";\nimport Swal from \"sweetalert2\";\nimport SideBar from \"./components/SdieBar.vue\";\nimport MainGame from \"./components/MainGame.vue\";\nimport Shop from \"./components/Shop.vue\";\nexport default {\n  name: \"App\",\n  components: {\n    SideBar: SideBar,\n    MainGame: MainGame,\n    Shop: Shop\n  },\n  data() {\n    return {\n      sign: 0,\n      name: \"\",\n      email: \"\",\n      password: \"\",\n      role: \"\"\n    };\n  },\n  mounted() {\n    this.$store.dispatch(\"getUserInfo\");\n  },\n  methods: {\n    showSign(bool) {\n      this.sign = bool;\n    },\n    login(event) {\n      event.preventDefault();\n      var loginData = {\n        email: this.email,\n        password: this.password\n      };\n      this.$store.dispatch(\"login\", loginData);\n    },\n    signup(event) {\n      event.preventDefault();\n      axios.post(\"/api/user/signup\", {\n        name: this.name,\n        email: this.email,\n        password: this.password,\n        role: \"USER\"\n      }).then(response => {\n        if (response.status == 200) {\n          Swal.fire({\n            title: \"회원가입성공\",\n            text: \"로그인 후 이용해주세요\",\n            icon: \"success\"\n          }).then(() => {\n            location.reload();\n          });\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","Swal","SideBar","MainGame","Shop","name","components","data","sign","email","password","role","mounted","$store","dispatch","methods","showSign","bool","login","event","preventDefault","loginData","signup","post","then","response","status","fire","title","text","icon","location","reload","catch","error","console","log"],"sources":["/Users/mungdori00/mun/vuemun/mun/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"text-center\" v-if=\"$store.state.isLogin == true\">{{ $store.state.userInfo }}의 개인화면</div>\n  <div class=\"wrapper\" v-if=\"$store.state.isLogin == false\">\n    <div class=\"login-container\" :class=\"{ 'right-panel-active': sign === 1 }\">\n      <div class=\"sign-up-container\">\n        <form>\n          <h1>Create Account</h1>\n          <div class=\"social-links\">\n            <div>\n              <a href=\"#\"><i class=\"fa fa-facebook\" aria-hidden=\"true\"></i></a>\n            </div>\n            <div>\n              <a href=\"#\"><i class=\"fa fa-twitter\" aria-hidden=\"true\"></i></a>\n            </div>\n            <div>\n              <a href=\"#\"><i class=\"fa fa-linkedin\" aria-hidden=\"true\"></i></a>\n            </div>\n          </div>\n          <span>or use your email for registration</span>\n          <input type=\"text\" placeholder=\"Name\" v-model=\"name\" />\n          <input type=\"email\" placeholder=\"Email\" v-model=\"email\" />\n          <input type=\"password\" placeholder=\"Password\" v-model=\"password\" />\n          <button class=\"form_btn\" @click=\"signup\">Sign Up</button>\n        </form>\n      </div>\n      <div class=\"sign-in-container\">\n        <form>\n          <h1>Sign In</h1>\n          <div class=\"social-links\">\n            <div>\n              <a href=\"#\"><i class=\"fa fa-facebook\" aria-hidden=\"true\"></i></a>\n            </div>\n            <div>\n              <a href=\"#\"><i class=\"fa fa-twitter\" aria-hidden=\"true\"></i></a>\n            </div>\n            <div>\n              <a href=\"#\"><i class=\"fa fa-linkedin\" aria-hidden=\"true\"></i></a>\n            </div>\n          </div>\n          <span>or use your account</span>\n          <input type=\"email\" placeholder=\"Email\" v-model=\"email\" />\n          <input type=\"password\" placeholder=\"Password\" v-model=\"password\" />\n          <button class=\"form_btn\" @click=\"login\">Sign In</button>\n        </form>\n      </div>\n      <div class=\"overlay-container\">\n        <div class=\"overlay-left\">\n          <h1>Welcome Back</h1>\n          <p>To keep connected with us please login with your personal info</p>\n          <button id=\"signIn\" @click=\"showSign(0)\" class=\"overlay_btn\">\n            Sign In\n          </button>\n        </div>\n        <div class=\"overlay-right\">\n          <h1>Hello, Friend</h1>\n          <p>Enter your personal details and start journey with us</p>\n          <button id=\"signUp\" @click=\"showSign(1)\" class=\"overlay_btn\">\n            Sign Up\n          </button>\n        </div>\n      </div>\n    </div>\n  </div>\n  <SideBar v-if=\"$store.state.isLogin == true\"></SideBar>\n  <MainGame v-if=\"$store.state.mainGame == true\"></MainGame>\n  <Shop v-if=\"$store.state.shop == true\"></Shop>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Swal from \"sweetalert2\";\nimport SideBar from \"./components/SdieBar.vue\";\nimport MainGame from \"./components/MainGame.vue\";\nimport Shop from \"./components/Shop.vue\";\nexport default {\n  name: \"App\",\n  components: {\n    SideBar: SideBar,\n    MainGame: MainGame,\n    Shop : Shop,\n  },\n  data() {\n    return {\n      sign: 0,\n      name: \"\",\n      email: \"\",\n      password: \"\",\n      role: \"\",\n    };\n  },\n  mounted() {\n    this.$store.dispatch(\"getUserInfo\");\n  },\n  methods: {\n    showSign(bool) {\n      this.sign = bool;\n    },\n    login(event) {\n      event.preventDefault();\n      var loginData = {\n        email: this.email,\n        password: this.password,\n      };\n      this.$store.dispatch(\"login\", loginData);\n    },\n    signup(event) {\n      event.preventDefault();\n      axios\n        .post(\"/api/user/signup\", {\n          name: this.name,\n          email: this.email,\n          password: this.password,\n          role: \"USER\",\n        })\n        .then((response) => {\n          if (response.status == 200) {\n            Swal.fire({\n              title: \"회원가입성공\",\n              text: \"로그인 후 이용해주세요\",\n              icon: \"success\",\n            }).then(() => {\n              location.reload();\n            });\n          }\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n  },\n};\n</script>\n\n<style>\n@import \"./assets/all.css\";\n</style>\n "],"mappings":"AAqEA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,aAAa;AAC9B,OAAOC,OAAM,MAAO,0BAA0B;AAC9C,OAAOC,QAAO,MAAO,2BAA2B;AAChD,OAAOC,IAAG,MAAO,uBAAuB;AACxC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVJ,OAAO,EAAEA,OAAO;IAChBC,QAAQ,EAAEA,QAAQ;IAClBC,IAAG,EAAIA;EACT,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,CAAC;MACPH,IAAI,EAAE,EAAE;MACRI,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,aAAa,CAAC;EACrC,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAACC,IAAI,EAAE;MACb,IAAI,CAACT,IAAG,GAAIS,IAAI;IAClB,CAAC;IACDC,KAAKA,CAACC,KAAK,EAAE;MACXA,KAAK,CAACC,cAAc,CAAC,CAAC;MACtB,IAAIC,SAAQ,GAAI;QACdZ,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,QAAQ,EAAE,IAAI,CAACA;MACjB,CAAC;MACD,IAAI,CAACG,MAAM,CAACC,QAAQ,CAAC,OAAO,EAAEO,SAAS,CAAC;IAC1C,CAAC;IACDC,MAAMA,CAACH,KAAK,EAAE;MACZA,KAAK,CAACC,cAAc,CAAC,CAAC;MACtBpB,KAAI,CACDuB,IAAI,CAAC,kBAAkB,EAAE;QACxBlB,IAAI,EAAE,IAAI,CAACA,IAAI;QACfI,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,IAAI,EAAE;MACR,CAAC,EACAa,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAIA,QAAQ,CAACC,MAAK,IAAK,GAAG,EAAE;UAC1BzB,IAAI,CAAC0B,IAAI,CAAC;YACRC,KAAK,EAAE,QAAQ;YACfC,IAAI,EAAE,cAAc;YACpBC,IAAI,EAAE;UACR,CAAC,CAAC,CAACN,IAAI,CAAC,MAAM;YACZO,QAAQ,CAACC,MAAM,CAAC,CAAC;UACnB,CAAC,CAAC;QACJ;MACF,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;QAChBC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC,CAAC;IACN;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}